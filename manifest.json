{
  "default_locale": "en",
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "version": "9",
  "applications": {
    "gecko": {
      "id": "quickfilterby@extensions.thunderbird.net",
      "strict_min_version": "115.0",
      "strict_max_version": "118.*"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "experiment_apis": {
    "MessagesListAdapter": {
      "schema": "api/MessagesListAdapter/schema.json",
      "parent": {
        "scopes": [
          "addon_parent"
        ],
        "paths": [
          [
            "MessagesListAdapter"
          ]
        ],
        "script": "api/MessagesListAdapter/implementation.js",
        "events": [
          "startup"
        ]
      }
    }
  },
  "permissions": [
    "menus",
    "messagesRead"
  ]
}
